@startchen cineplanet
left to right direction
entity Ciudad {
    id_ciudad : INT <<key>>
    nombre : STRING
}
entity Cine {
    id_ciudad : INT FK
    id_cine : INT <<key>>
}
relationship Se_encuentra {
}

Cine -N- Se_encuentra
Se_encuentra -1- Ciudad

entity Sala {
    id_cine : INT FK
    id_sala : INT <<key>>
    filas : INT
    columnas : INT
}

relationship Contiene {
}

Sala -N- Contiene
Contiene -1- Cine

entity Formato {
    id_formato : INT <<key>>
    nombre : STRING
}

relationship Funciona {
}

Sala -N- Funciona
Funciona -1- Formato

entity Asiento {
    id_sala : INT PK
    fila : INT PK
    columna : INT PK
}

relationship Tiene {
}

Sala -1- Tiene
Tiene -N- Asiento

entity FuncionAsiento {
    id_funcionsala : INT FK
    fila : INT PK
    columna : INT PK
    estado : INT FK
}
entity Dulceria {
    id_cine : INT FK
    id_dulceria : INT <<key>>
}
entity DulceriaCategoria {
    id_dulceria : INT PK
    id_categoria : INT PK
}
entity Categoria {
    id_categoria : INT <<key>>
    nombre : STRING
}
entity Dulce {
    id_categoria : INT FK
    id_dulce : INT <<key>>
    nombre : STRING
    precio : INT
}
entity Pelicula {
    id_pelicula : INT <<key>>
    nombre : STRING
    duracion : INT
    id_formato : INT FK
}
entity Funcion {
    id_funcion : INT <<key>>
    hora_inicio : STRING
    id_pelicula : INT FK
}
entity Cliente {
    nombre : STRING
    dni : STRING
    id_funcion_asiento : INT PK
}
entity Miembro {
    id_miembro : INT <<key>>
    nombre : STRING
    dni : STRING
    nivel : INT FK
    entradas : INT
    fecha_cumple : STRING
    id_cliente : INT FK
}
entity Personal {
    id_personal : INT <<key>>
    nombre : STRING
    dni : STRING
    celular : STRING
    id_trabajo : INT FK
}
entity Trabajo {
    id_trabajo : INT <<key>>
    nombre : STRING
}
entity FuncionSala {
    id_funcion : INT PK
    id_sala : INT PK
}
@endchen