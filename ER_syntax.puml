@startchen cineplanet
entity Ciudad {
    id_ciudad : INT <<key>>
    nombre : STRING
}
entity Cine {
    id_ciudad : INT FK
    id_cine : INT <<key>>
}
entity Sala {
    id_cine : INT FK
    id_sala : INT <<key>>
    filas : INT
    columnas : INT
}
entity Asiento {
    id_sala : INT FK
    fila : INT PK
    columna : INT PK
}
entity FuncionAsiento {
    id_funcion : INT FK
    fila : INT PK
    columna : INT PK
    estado : INT FK
}
entity Dulceria {
    id_cine : INT FK
    id_dulceria : INT <<key>>
}
entity Categoria {
    id_categoria : INT <<key>>
    nombre : STRING
}
entity Dulce {
}
entity Pelicula {
    id_pelicula : INT <<key>>
    nombre : STRING
    duracion : INT
    id_formato : INT FK
}
entity Funcion {
    id_funcion : INT <<key>>
    hora_inicio : STRING
    id_pelicula : INT FK
}
entity Cliente {
    nombre : STRING
    dni : STRING
    id_funcion_asiento : INT PK
}
entity Miembro {
    id_miembro : INT <<key>>
    nombre : STRING
    dni : STRING
    nivel : INT FK
    entradas : INT
    fecha_cumple : STRING
    id_cliente : INT FK
}
entity Personal {
}
entity FuncionSala {
    id_funcion : INT PK
    id_sala : INT PK
}
@endchen